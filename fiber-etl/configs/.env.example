# Fiber-ETL Configuration
# Copy this to .env and update with your values

# Environment
ENV=dev

# Redis Configuration
REDIS_URL=redis://localhost:6379/0

# Database Configuration
DB_HOST=localhost
DB_PORT=5432
DB_USER=postgres
DB_PASS=postgres
DB_NAME=fiberstack

# Elasticsearch Configuration
ELASTIC_URL=http://localhost:9200
ELASTIC_INDEX_PREFIX=fiber-logs

# Processing Configuration
BATCH_SIZE=100
WORKER_THREADS=2

# Logging
LOG_LEVEL=INFO
