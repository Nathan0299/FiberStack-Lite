# Fiber-API Configuration
# Copy this to .env and update with your values

# Environment
ENV=dev

# Redis Configuration
REDIS_URL=redis://localhost:6379/0

# Database Configuration
DB_HOST=localhost
DB_PORT=5432
DB_USER=postgres
DB_PASS=postgres
DB_NAME=fiberstack

# API Configuration
API_HOST=0.0.0.0
API_PORT=8000
CORS_ORIGINS=http://localhost:4000,http://localhost:3000

# Logging
LOG_LEVEL=INFO

# Security (generate in production)
API_KEY_SECRET=CHANGE_THIS_IN_PRODUCTION
